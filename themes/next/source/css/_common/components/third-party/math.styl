mjx-container[jax="CHTML"][display="true"], .has-jax {
  overflow: auto hidden;
}
mjx-container + br {
  display: none;
}

.gt-container .gt-btn {
    border: 1px solid #f4a7b9;
    background-color: #fedfe1;
}

.gt-container a {
    color: #f4a7b9;
}

.gt-container .gt-link {
    border-bottom: 1px dotted #f4a7b9;
}

.gt-container .gt-svg svg {
    fill: #f4a7b9;
}